

	<% for (var x= 0; x< hoteis.length; x++	) { %>
		<div class="infos row align-middle">
			<img src="<%= hoteis[x].img %>" class="imagem large-3 columns ">
			<div class="large-9 columns">
				<h4 class="title"><%= hoteis[x].name %></h4>
				<p><%= hoteis[x].description %></p>
				<a href="<%=hoteis[x].link%>">
					<%= hoteis[x].link%>
				</a>
				<p class="text">
					<%= hoteis[x].location %>
				</p>
					<a target="_blank" href="https://web.moovitapp.com/tripplan?metroId=242&lang=pt" >
						<img src="/images/moovit.png" class="infocaminho">
					</a>
			</div>

		</div>
	

	 <% } %>

